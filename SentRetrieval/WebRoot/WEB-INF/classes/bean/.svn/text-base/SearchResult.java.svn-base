package bean;

public class SearchResult {
	public String Sent;
	public int SentIndex;
	public SearchResult(String sent,int sentindex){
		this.Sent = sent;
		this.SentIndex = sentindex;
	}
	public String getSent(){
		return this.Sent;
	}
	public int getSentIndex(){
		return this.SentIndex;
	}
}
